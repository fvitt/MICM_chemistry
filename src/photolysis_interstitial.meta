  units = count
  dimensions = ()
  type = integer
  intent = in
[errmsg]
  standard_name = ccpp_error_message
  long_name = CCPP error message
  units = 1
  dimensions = ()
  type = character
  kind = len=512
  intent = out
[errflg]
  standard_name = ccpp_error_flag
  long_name = CCPP error flag
  units = flag
  dimensions = ()
  type = integer
  intent = out

